{
  "pages": [
    "pages/index/index",
    "pages/editor/editor",
    "pages/courses/courses",
    "pages/projects/projects",
    "pages/profile/profile",
    "pages/course-detail/course-detail",
    "pages/project-detail/project-detail",
    "pages/blockly/blockly"
  ],
  "window": {
    "navigationBarBackgroundColor": "#1E5A8E",
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "丝路智星",
    "backgroundColor": "#F5F5F5",
    "backgroundTextStyle": "light",
    "enablePullDownRefresh": true
  },
  "tabBar": {
    "color": "#666666",
    "selectedColor": "#1E5A8E",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "assets/tab-home.png",
        "selectedIconPath": "assets/tab-home-active.png"
      },
      {
        "pagePath": "pages/courses/courses",
        "text": "课程",
        "iconPath": "assets/tab-course.png",
        "selectedIconPath": "assets/tab-course-active.png"
      },
      {
        "pagePath": "pages/editor/editor",
        "text": "创作",
        "iconPath": "assets/tab-create.png",
        "selectedIconPath": "assets/tab-create-active.png"
      },
      {
        "pagePath": "pages/projects/projects",
        "text": "作品",
        "iconPath": "assets/tab-project.png",
        "selectedIconPath": "assets/tab-project-active.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "text": "我的",
        "iconPath": "assets/tab-profile.png",
        "selectedIconPath": "assets/tab-profile-active.png"
      }
    ]
  },
  "permission": {
    "scope.userLocation": {
      "desc": "获取您的位置信息用于推荐附近的编程活动"
    }
  },
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000
  },
  "plugins": {
    "blockly": {
      "version": "1.0.0",
      "provider": "wxidxxxxxxxxxxxxx"
    }
  },
  "sitemapLocation": "sitemap.json",
  "style": "v2",
  "lazyCodeLoading": "requiredComponents",
  "renderer": "skyline",
  "componentFramework": "glass-easel",
  "usingComponents": {},
  "requiredPrivateInfos": [
    "chooseLocation",
    "getLocation"
  ]
}
